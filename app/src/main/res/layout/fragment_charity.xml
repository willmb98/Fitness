<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/charitytxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginRight="8dp"
        android:gravity="center_horizontal"
        android:text="Charity Sign up! get fit while showing support to the less fortunate"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/CharitySignUp1"
        android:layout_width="153dp"
        android:layout_height="122dp"
        android:layout_marginStart="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/charitytxt"
        app:srcCompat="@drawable/battlecancer" />

    <ImageView
        android:id="@+id/CharitySignUp3"
        android:layout_width="153dp"
        android:layout_height="122dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="@+id/CharitySignUp1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/CharitySignUp1"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp1"
        app:srcCompat="@drawable/calm" />

    <ImageView
        android:id="@+id/CharitySignUp5"
        android:layout_width="153dp"
        android:layout_height="122dp"
        android:layout_marginTop="56dp"
        app:layout_constraintEnd_toEndOf="@+id/CharitySignUp3"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/CharitySignUp3"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp3"
        app:srcCompat="@drawable/cruk" />

    <ImageView
        android:id="@+id/CharitySignUp6"
        android:layout_width="153dp"
        android:layout_height="122dp"
        app:layout_constraintEnd_toEndOf="@+id/CharitySignUp4"
        app:layout_constraintStart_toStartOf="@+id/CharitySignUp4"
        app:layout_constraintTop_toTopOf="@+id/CharitySignUp5"
        app:srcCompat="@drawable/gnr" />

    <ImageView
        android:id="@+id/CharitySignUp2"
        android:layout_width="153dp"
        android:layout_height="122dp"
        android:layout_marginEnd="16dp"
        android:layout_marginRight="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/CharitySignUp1"
        app:srcCompat="@drawable/cwc" />

    <ImageView
        android:id="@+id/CharitySignUp4"
        android:layout_width="153dp"
        android:layout_height="122dp"
        app:layout_constraintEnd_toEndOf="@+id/CharitySignUp2"
        app:layout_constraintStart_toStartOf="@+id/CharitySignUp2"
        app:layout_constraintTop_toTopOf="@+id/CharitySignUp3"
        app:srcCompat="@drawable/rfc" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="12dp"
        android:text="Battle Cancer 2020"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp1" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Children with Cancer"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.796"
        app:layout_constraintStart_toEndOf="@+id/textView"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp2" />

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="44dp"
        android:layout_marginLeft="44dp"
        android:layout_marginTop="12dp"
        android:text="The Home Run"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp3" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:text="Run For Charity"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.742"
        app:layout_constraintStart_toEndOf="@+id/textView12"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp4" />

    <TextView
        android:id="@+id/textView14"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginLeft="32dp"
        android:layout_marginTop="12dp"
        android:text="Cancer Research Uk"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp5" />

    <TextView
        android:id="@+id/textView15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="112dp"
        android:layout_marginLeft="112dp"
        android:layout_marginTop="12dp"
        android:text="Time Outdoors"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/textView14"
        app:layout_constraintTop_toBottomOf="@+id/CharitySignUp6" />

</androidx.constraintlayout.widget.ConstraintLayout>